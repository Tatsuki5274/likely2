<!DOCTYPE html>
<html>
  <head>
    <title>likely - admin</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
<body>
	<header>
    <div class="container clearfix">
    	<div class="admin-header-title"><%= link_to "Likely:admin", admin_path %></div>
	    <nav class="admin-header-nav">
	        <ul>
	            <li class="header-nav-list"><%= link_to " Products ", admin_products_path %></li>
	            <li class="header-nav-list"><%= link_to " Users ", admin_users_path %></li>
	            <li class="header-nav-list"><%= link_to " Orders ", admin_orders_path %></li>
	        </ul>
	    </nav>
	</div>
	</header>
  <div class="container clearfix">
    <% content_for :content do %>
      <%= yield %>
    <% end %>
    <%= render template: "layouts/application" %>
  </div>
<footer>
  <div class="footer-box">
    <div class="footer-text">
      <%= link_to "お問い合わせ一覧", admin_contacts_path %>
    </div>
  </div>
</footer>
</body>
</html>